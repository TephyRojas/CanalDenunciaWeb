<form id="frmRecupera">
    <div class="row">
        <div class="col-md-4 offset-md-4 col-sm-12">
            <div class="card border-primary mb-3">
                <div class="card-header">Recuperar Contraseña</div>
                <div class="card-body">
                    <div class="form-group" id="frmContrasena">
                        <label for="Rut">Ingrese RUT</label>
                        <div class="input-group mb-3">
                            <input type="text" class="form-control" id="Rut" name="Rut" required="" autocomplete="off">
                        </div>
                        <div class="invalid-feedback" id="msjRut"></div>
                    </div>

                </div>
                <div class="card-footer">
                    <div class="row">
                        <div class="col-md-12">
                            <button id="btnRecupera" name="btnRecupera" class="btn btn-danger btn-block"><i class="fas fa-key"></i>&nbsp;Recuperar</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>
<script src="~/Scripts/jquery-3.5.1.js"></script>
<script src="~/Scripts/bootstrap.js"></script>
<script type="text/javascript" src="~/Scripts/sweetalert2.all.min.js"></script>
